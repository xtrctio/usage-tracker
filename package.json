{
  "name": "@xtrctio/usage-tracker",
  "description": "Every request counts",
  "version": "1.0.5",
  "author": "Eric Hacke",
  "bugs": {
    "url": "https://github.com/xtrctio/usage-tracker/issues"
  },
  "dependencies": {
    "@xtrctio/common": "^6.0.9",
    "@xtrctio/redis": "^1.1.5",
    "@xtrctio/xerror": "^1.0.2",
    "ajv": "^6.10.0",
    "ajv-errors": "^1.0.0",
    "bluebird": "^3.5.3",
    "bunyan": "^1.8.12",
    "bunyan-prettystream": "^0.1.3",
    "ioredis": "^4.6.2",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.10",
    "luxon": "^1.11.4"
  },
  "devDependencies": {
    "@xtrctio/eslint-config": "^3.0.5",
    "chai": "^4.1.2",
    "dotenv": "^7.0.0",
    "eslint": "^5.15.1",
    "firebase-admin": "^7.1.0",
    "firebase-tools": "^6.5.0",
    "husky": "^1.3.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "sinon": "^7.2.7"
  },
  "files": [
    "lib/**",
    "config/**",
    "app/**",
    "*.js",
    "!scratch.*"
  ],
  "homepage": "https://github.com/xtrctio/usage-tracker#readme",
  "husky": {
    "hooks": {
      "pre-commit": "fixpack && npm run docs && LIST=`git diff-index --cached --name-only HEAD | grep '.*\\.js' | grep -v json`; if [ \"$LIST\" ]; then npm run lint && git add $LIST; fi"
    }
  },
  "keywords": [
    "api",
    "tracking",
    "usage"
  ],
  "license": "ISC",
  "main": "index.js",
  "nyc": {
    "exclude": [
      "coverage/**",
      "tests/**",
      "scratch.js",
      "index.js"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary",
      "lcov"
    ],
    "all": true
  },
  "peerDependencies": {
    "firebase-admin": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtrctio/usage-tracker.git"
  },
  "scripts": {
    "docs": "jsdoc2md --template README.template --files \"lib/**/*.js\" > README.md && git add README.md",
    "internet": "mocha \"tests/**/*.internet.js\"",
    "lint": "eslint \"**/*.js\" --fix",
    "test": "npm run unit && npm run lint",
    "test-all": "nyc mocha --use_strict \"tests/**/*.internet.js\" \"tests/**/*.it.js\" \"tests/**/*.unit.js\"",
    "test-most": "nyc mocha --use_strict \"tests/**/*.it.js\" \"tests/**/*.unit.js\"",
    "unit": "nyc mocha --use_strict --exit \"tests/**/*.unit.js\"",
    "update-classes": "node updateClasses.js && git add lib/models/AUTOGENERATED-classMap.json"
  }
}
